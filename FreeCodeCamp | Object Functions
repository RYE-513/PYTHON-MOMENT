# Student.py
Class Student: # "self" is actual object

def __ init ___(self, name, major, gpa):
           self.name = name
           self.major = major
           self.gpa = gpa

def on_honor_roll(self):
       if self.gpa > 3.5:
          return True
       else:
          return False
