inventory = ['Axe', 'Great Sword', 'Stick']
rarity = {
       'Great Sword' : 98,
       'Golden Bow, : 97,
       'Iron Sword' : 80,
       'Axe' : 34,
       'Stick' : 1}

# sort_inv = (inventory, \key=rarity.__getitem__, reverse = True)

sort_inv = sorted(inventory, key=rarity.__get__item)
print(sort_inv)
