#. COUNT DOWN

countNumber = 10
lastNumber = 0

while countNumber >= lastNumber:
   print(countNumber)
   countNumber = countNumber -1

#. COUNT UP

countNumber = 0
lastNumber = 10

while countNumber <= lastNumber :
   print(countNumber)
   countNumber = countNumber +1 # or countNumber+=1


numX = int(input())
while numX = 0 :
    print(numX)
    numX = numX -1

count = 1

while count <= numX :
    print(count)
    count = count+1
