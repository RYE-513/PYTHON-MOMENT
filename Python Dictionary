
d = {}
# d = {"Ryan": 21, "John": 19}

d["Ryan"] = 21
d["John"] = 19
d["Rye"] = 23

print(d["Ryan"])
print(d["John"])
print(d["Rye"])

d["Ryan"] = 513 #Updated
print(d["Ryan"])

d[10] = 100
print(d[10])

# ITERATE OVER KEY-VALUE PAIRS

for key, value in d.items():
    print("KEY: ")
    print(key)
    print("VALUE: ")
    print(value)
    print()


